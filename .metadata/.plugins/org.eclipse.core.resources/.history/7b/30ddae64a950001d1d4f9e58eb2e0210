package Overriding;


class Vehicle{
	void run() {
		System.out.println("Vehicle is running....");
	}
}

public class Bike extends Vehicle {
	
	void run() {
		Sytem.out.println("Bike is running fast")
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Bike b=new Bike();
		b.run();
	}

}
